<h1>Editing Book</h1>
<%= form_with model: @book, url: "/bookers/#{@book.id}", local:true do |f| %>
<br>Title</br>
<%= f.text_field :title %>
<br>Body</br>
<%= f.text_area :body %>
<%= f.submit"Update Book" %>

<p><%= link_to "show", "/bookers/#{@book.id}" %>|<%= link_to "Back", bookers_path %></p>
<% end %>
